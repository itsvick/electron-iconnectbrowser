{
  "name": "iconnectbrowser",
  "title": "iConnect Browser Desktop",
  "description": "iConnect Browser Desktop",
  "version": "1.0.0",
  "main": "main.js",
  "private": true,
  "author": {
    "name": "iConnect Team",
    "email": "ali@icycle-global.com.com"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:build:windows": "npm run build:prod && node copyAssets.js && electron-builder build --windows --publish=never",
    "rebuild": "electron-rebuild -f -w drivelist",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "build:start:dev": "npm run build:dev && npx electron .",
    "build:start:prod": "npm run build:dev && npx electron .",
    "start:built": "npx electron .",
    "build-electron": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --base-href ./ --outputPath=./dist",
    "build-icons": "electron-icon-maker --input=src/assets/favicon.png --output=./build",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=dist/assets/favicon.icns --prune=true --out=release-builds/mac",
    "build-package": "npm run build-electron --prefix .",
    "installer-mac": "electron-installer-dmg ./release-builds/mac/iconnectdesktop-darwin-x64/iconnectdesktop.app iconnect-desktop --out=release-builds/mac/installer --overwrite --icon=dist/assets/favicon.icns",
    "complete-mac": "npm run build-package && npm run package-mac && npm run installer-mac"
  },
  "dependencies": {
    "@electron/remote": "1.0.4",
    "drivelist": "4.0.0",
    "electron-installer-dmg": "^3.0.0",
    "usb-detection": "4.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.1",
    "@angular-devkit/build-angular": "0.1102.5",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/animations": "10.1.1",
    "@angular/cdk": "^11.2.5",
    "@angular/cli": "11.2.5",
    "@angular/common": "11.2.6",
    "@angular/compiler": "11.2.6",
    "@angular/compiler-cli": "11.2.6",
    "@angular/core": "11.2.6",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.6",
    "@angular/language-service": "11.2.6",
    "@angular/material": "11.0.0",
    "@angular/platform-browser": "11.2.6",
    "@angular/platform-browser-dynamic": "11.2.6",
    "@angular/router": "11.2.6",
    "@auth0/angular-jwt": "4.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.6.7",
    "@types/jasminewd2": "2.0.8",
    "@types/mathjax": "0.0.36",
    "@types/mocha": "8.2.1",
    "@types/node": "14.14.35",
    "@types/usb": "^1.5.2",
    "@types/webpack": "4.32.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.15.0",
    "@typescript-eslint/parser": "4.16.1",
    "angular-google-tag-manager": "1.1.4",
    "chai": "4.3.4",
    "codelyzer": "5.1.2",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.6.5",
    "cross-env": "7.0.3",
    "electron": "12.0.1",
    "electron-builder": "22.10.5",
    "electron-icon-maker": "0.0.5",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^3.2.3",
    "eslint": "7.22.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "file-saver": "2.0.5",
    "fs-extra": "^10.0.0",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-rc.7",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "jspdf": "1.5.3",
    "karma": "6.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lodash": "^4.17.21",
    "mathjax": "3.1.2",
    "mocha": "8.3.2",
    "moment": "2.29.1",
    "ng2-pdf-viewer": "^6.4.0",
    "npm-run-all": "4.1.5",
    "protractor": "7.0.0",
    "rxjs": "6.6.6",
    "spectron": "14.0.0",
    "ts-loader": "8.0.3",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.0.5",
    "wait-on": "5.0.1",
    "webdriver-manager": "12.1.8",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "zone.js": "0.10.3"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "chrome 83"
  ]
}
